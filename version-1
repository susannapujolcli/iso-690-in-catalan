Sub replace_Wikipedia()
'
'
' replace_Wikipedia Macro
'
'
    Selection.Find.ClearFormatting
        Selection.Find.Replacement.ClearFormatting
        With Selection.Find
            .Text = "Wikipedia"
            .Replacement.Text = "Viquipèdia"
            .Forward = True
            .Wrap = wdFindContinue
            .Format = False
            .MatchCase = False
            .MatchWholeWord = False
            .MatchWildcards = False
            .MatchSoundsLike = False
            .MatchAllWordForms = False
        End With
        Selection.Find.Execute Replace:=wdReplaceAll
End Sub

Sub replace_ISOes_Bibliography()
    
    replace_sinfecha
    replace_enlinea
    replace_mesenero
    replace_mesfebrero
    replace_mesmarzo
    replace_mesmayo
    replace_mesjunio
    replace_mesjulio
    replace_mesagosto
    replace_messeptiembre
    replace_mesnoviembre
    replace_mesdiciembre
    replace_disponibleen
    
End Sub
